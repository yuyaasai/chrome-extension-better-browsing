{
    "name": "better-browsing",
    "version": "3.0.1",
    "description": "my backstage chrome extension for better browsing",
    "main": "index.js",
    "scripts": {
        "initialize": "npm ci && git submodule update --init && git submodule sync && npm run build",
        "build": "npm audit && webpack --config webpack/webpack.config.js",
        "watch": "npm audit && webpack --config webpack/webpack.config.js --watch",
        "testwatch": "jest --watch",
        "test": "jest"
    },
